{
  "name": "cl-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "loki:ci": "yarn run storybook:build; yarn run loki:test",
    "loki:test": "loki test --requireReference --chromeTolerance 10 --reactUri file:./storybook-static; yarn run loki:report",
    "loki:report": "yarn run reg-cli ./.loki/current ./.loki/reference ./.loki/diff --extendedErrors --enableAntialias --matchingThreshold .2 --report ./.loki/index.html --json ./.loki/report.json",
    "loki:update": "loki update; yarn run reg-cli:update;",
    "reg-cli:update": "yarn run reg-cli ./.loki/current ./.loki/reference ./.loki/diff --json ./.loki/report.json -U"
  },
  "dependencies": {
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "^7.3.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.3.2",
    "@storybook/addon-styling": "^1.3.6",
    "@storybook/blocks": "^7.3.2",
    "@storybook/nextjs": "^7.3.2",
    "@storybook/react": "^7.3.2",
    "@testing-library/jest-dom": "^6.0.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest-axe": "^3.5.5",
    "@types/node": "20.5.1",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "classnames": "^2.3.2",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-storybook": "^0.6.13",
    "husky": "^8.0.0",
    "jest": "^29.6.3",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.6.3",
    "loki": "^0.32.0",
    "prettier": "^3.0.2",
    "react-intersection-observer": "^9.5.2",
    "reg-cli": "^0.17.7",
    "sass": "^1.66.1",
    "storybook-css-modules-preset": "^1.1.1",
    "typescript": "5.1.6"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  }
}